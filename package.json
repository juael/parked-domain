{
  "name": "parked-domain",
  "version": "0.0.1",
  "private": true,
  "description": "Parked Domain",
  "scripts": {
    "build:elm": "elm make src/Main.elm --optimize --output=./dist/elm.js",
    "build:css": "tailwindcss -i ./style.css -o ./dist/style.css --minify",
    "build:html": "cp index.html dist/index.html",
    "build": "rm -rf dist && mkdir dist && pnpm build:elm && pnpm build:css && pnpm build:html",
    "start": "elm-live src/Main.elm --port 8010 --pushstate -- --output=elm.js",
    "format": "elm-format src --yes",
    "review": "elm-review",
    "test": "elm-test"
  },
  "keywords": [
    "elm",
    "tailwindcss"
  ],
  "author": "Julian Dawson",
  "license": "ISC",
  "packageManager": "pnpm@10.19.0",
  "engines": {
    "node": ">=20"
  },
  "devDependencies": {
    "@tailwindcss/cli": "4.1.18",
    "elm": "0.19.1-6",
    "elm-format": "0.8.8",
    "elm-live": "4.0.2",
    "elm-json": "0.2.13",
    "elm-review": "^2.13.5",
    "elm-test": "0.19.1-revision17",
    "tailwindcss": "^4.1.18"
  }
}
